odoo.define('@web/core/dropdown/accordion_item', ['@odoo/owl'], function (require) {
'use strict';
let __exports = {};
const { Component, onPatched, useState } = require("@odoo/owl");

const ACCORDION = __exports.ACCORDION = Symbol("Accordion");
