/*************************************************
*  Filepath: /web/static/src/polyfills/array.js  *
*  Lines: 12                                     *
*************************************************/
// @odoo-module ignore
if (!Array.prototype.at) {
    Object.defineProperty(Array.prototype, "at", {
        enumerable: false,
        value: function (index) {
            if (index >= 0) {
                return this[index];
            }
            return this[this.length + index];
        }
    });
}
