odoo.define('@web/../lib/hoot-dom/hoot-dom', ['@web/../lib/hoot-dom/helpers/dom', '@web/../lib/hoot-dom/helpers/events', '@web/../lib/hoot-dom/hoot_dom_utils', '@web/../lib/hoot-dom/helpers/time'], function (require) {
